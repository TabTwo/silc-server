	        && { if test -f $@; then exit 0; else break; fi; }; \
	      exit 1;; \
	  esac; \
	done; \
	echo ' cd $(top_srcdir) && $(AUTOMAKE) --foreign  apps/silcd/Makefile'; \
	cd $(top_srcdir) && \
	  $(AUTOMAKE) --foreign  apps/silcd/Makefile
